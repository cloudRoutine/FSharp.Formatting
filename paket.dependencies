    source https://api.nuget.org/v3/index.json
    source https://dotnet.myget.org/F/roslyn-tools/api/v3/index.json
    nuget Paket.Core 
    nuget NuGet.CommandLine
    nuget RoslynTools.ReferenceAssemblies


